{
  "name": "Python 3.10 Devcontainer",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance",
        "dbaeumer.vscode-eslint"
      ]
    }
  },
  "runArgs": ["--env-file",".env"],
  "postCreateCommand": "bash -l /usr/local/bin/install.sh"
}
